<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.an.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/an_48px"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityDF"
            android:label="测向罗盘"></activity>
        <activity
            android:name=".ActivityGeneralSpectrumView"
            android:label="通用频谱图控件" />
        <activity
            android:name=".ActivityWaterfullView"
            android:label="瀑布图" />
        <activity
            android:name=".ActivitySpectrumView"
            android:label="频谱图" />
        <activity
            android:name=".ActivityLevelStreamView"
            android:label="电平流" />
        <activity
            android:name=".ActivityProgressBar"
            android:label="进度条" />
        <activity
            android:name=".ActivityCompassView"
            android:label="示向度" />
        <activity
            android:name=".ActivityScaleBar"
            android:label="刻度条" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>